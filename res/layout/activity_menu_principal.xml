<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_image"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <TextView
        android:id="@+id/textView_menu_principal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="05dp"
        android:text="@string/menuprincipal"
        android:textColor="@color/Black"
        android:textColorLink="@color/White"
        android:textSize="@dimen/tamanhoDaFonte" />

	<RelativeLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:background="@color/Black"
	    android:paddingBottom="@dimen/activity_vertical_margin"
	    android:paddingLeft="@dimen/activity_horizontal_margin"
	    android:paddingRight="@dimen/activity_horizontal_margin"
	    android:paddingTop="@dimen/activity_vertical_margin" >
	    
	    <Button
	        android:id="@+id/menu_principal_gerenciar_vendas"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/menu_principal_gerenciar_contas"
	        android:layout_below="@+id/menu_principal_gerenciar_contas"
	        android:layout_marginTop="22dp"
	        android:onClick="GerenciarVendas"
	        android:text="@string/gerenciarVendas" />
	
	    <Button
	        android:id="@+id/menu_principal_gerenciar_cliente"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_below="@+id/textView_menu_principal"
	        android:layout_centerHorizontal="true"
	        android:layout_marginTop="19dp"
	        android:text="@string/gerenciarCliente" 
	        android:onClick="GerenciarCliente"/>
	
	    <TextView
	        android:id="@+id/textView_menu_principal"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentTop="true"
	        android:layout_centerHorizontal="true"
	        android:layout_marginTop="05dp"
	        android:text="@string/menuprincipal"
	        android:textColor="@color/White"
	        android:textColorLink="@color/White"
	        android:textSize="@dimen/tamanhoDaFonte" />
	
	    <Button
	        android:id="@+id/menu_principal_gerenciar_vendedor"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/menu_principal_gerenciar_cliente"
	        android:layout_below="@+id/menu_principal_gerenciar_cliente"
	        android:layout_marginTop="16dp"
	        
	        android:text="@string/gerenciarVendedor" />
	
	    <Button
	        android:id="@+id/menu_principal_gerenciar_produto"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/menu_principal_gerenciar_vendedor"
	        android:layout_below="@+id/menu_principal_gerenciar_vendedor"
	        android:layout_marginTop="16dp"
	        android:text="@string/gerenciarProduto" />
	
	    <Button
	        android:id="@+id/menu_principal_gerenciar_contas"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/menu_principal_gerenciar_produto"
	        android:layout_below="@+id/menu_principal_gerenciar_produto"
	        android:layout_marginTop="16dp"
	        android:text="@string/gerenciarContas" />

	
    <ImageButton
        android:id="@+id/imageButtonGerenciarContasTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonGerenciarVendedorTelaMenuPrincipal"
        android:layout_below="@+id/imageButtonGerenciarVendedorTelaMenuPrincipal"
        android:layout_marginTop="35dp"
        android:src="@drawable/contas" 
        android:onClick="GerenciarContas"/>

    <ImageButton
        android:id="@+id/imageButtonGerenciarProdutoTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageButtonGerenciarClienteTelaMenuPrincipal"
        android:layout_alignTop="@+id/imageButtonGerenciarContasTelaMenuPrincipal"
        android:src="@drawable/produto"
        android:onClick="GerenciarProduto" />

    <ImageButton
        android:id="@+id/imageButtonConfiguracoesTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonGerenciarContasTelaMenuPrincipal"
        android:layout_alignTop="@+id/imageButtonVendasTelaMenuPrincipal"
        android:src="@drawable/configuracoes" />

    <TextView
        android:id="@+id/textViewBuscarClienteGerenciarCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/menu_principal_gerenciar_cliente"
        android:layout_below="@+id/menu_principal_gerenciar_cliente"
        android:layout_marginTop="16dp"
        android:onClick="GerenciarVendedor"
        android:text="@string/gerenciarVendedor" />


    <TextView
        android:id="@+id/textViewNovoVendedorGerenciarVendedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonGerenciarVendedorTelaMenuPrincipal"
        android:layout_alignRight="@+id/imageButtonGerenciarClienteTelaMenuPrincipal"
        android:text="@string/gerenciarCliente" />

    <ImageButton
        android:id="@+id/imageButtonGerenciarClienteTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/menu_principal_gerenciar_produto"
        android:layout_below="@+id/menu_principal_gerenciar_produto"
        android:layout_marginTop="16dp"
        android:text="@string/gerenciarContas" 
        android:onClick="GerenciarContas"/>


    <ImageButton
        android:id="@+id/imageButtonGerenciarVendedorTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView_menu_principal"
        android:layout_alignTop="@+id/imageButtonGerenciarClienteTelaMenuPrincipal"
        android:src="@drawable/vendedor" 
        android:onClick="GerenciarVendedor"/>

    <TextView
        android:id="@+id/textViewAtualizarClienteGerenciarCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewNovoVendedorGerenciarVendedor"
        android:layout_alignBottom="@+id/textViewNovoVendedorGerenciarVendedor"
        android:layout_alignRight="@+id/imageButtonGerenciarVendedorTelaMenuPrincipal"
        android:text="@string/gerenciarVendedor" />

    <TextView
        android:id="@+id/textViewExcluirClienteGerenciarCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewBuscarClienteGerenciarCliente"
        android:layout_alignBottom="@+id/textViewBuscarClienteGerenciarCliente"
        android:layout_alignLeft="@+id/imageButtonGerenciarContasTelaMenuPrincipal"
        android:text="@string/gerenciarContas" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonConfiguracoesTelaMenuPrincipal"
        android:layout_alignParentRight="true"
        android:layout_marginRight="38dp"
        android:text="@string/gerenciarConfiguracoes" />

    <ImageButton
        android:id="@+id/imageButtonVendasTelaMenuPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonGerenciarProdutoTelaMenuPrincipal"
        android:layout_below="@+id/imageButtonGerenciarProdutoTelaMenuPrincipal"
        android:layout_marginTop="36dp"
        android:src="@drawable/dinheiro" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonConfiguracoesTelaMenuPrincipal"
        android:layout_alignLeft="@+id/textViewBuscarClienteGerenciarCliente"
        android:text="@string/gerenciarVendas" />
</RelativeLayout>
</RelativeLayout>