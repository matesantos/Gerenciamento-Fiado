<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollViewAtualizarCliente"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="16dp"
    tools:context=".AtualizarVendedorActivity"    
    >
    
    <LinearLayout
        android:id="@+id/LinearLayoutTelaAtualizarVendedor1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_image"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin" >

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewDadosPessoaisVendedor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="55dip"
                android:layout_marginTop="5dip"
                android:orientation="vertical"
                android:text="@string/dadosPessoaisVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/Red"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewNomeTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/nomeVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextNomeVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >
                <requestFocus />
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewRGVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/rgVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextRGVendedorTelaAtualizarVendedor"
                android:layout_width="83dp"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>

            <TextView
                android:id="@+id/textViewCPFVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/cpfVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <br.ufpb.lp3.gerenciamento_fiado.utils.CpfMaskEditText
                android:id="@+id/editTextCPFVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewTelefoneVendedorTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/telefoneVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextTelefoneVendedorTelaAtualizarVendedor"
                android:layout_width="0dip"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:layout_weight="0.20"
                android:ems="10"
                android:inputType="text" >

            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewLoginVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/loginVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextLoginVendedorTelaAtualizarVendedor"
                android:layout_width="83dp"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>

            <TextView
                android:id="@+id/textViewSenhaVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/senhaVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextSenhaVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>
        </LinearLayout>

        <!-- Parte 2 do layout -->

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewDadosEnderecoVendedorTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="100dip"
                android:layout_marginTop="5dip"
                android:orientation="vertical"
                android:text="@string/dadosEnderecoVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/Red"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewEnderecoTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/enderecoVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextEnderecoVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewNumeroVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/numeroCasaVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextNumeroVendedorTelaAtualizarVendedor"
                android:layout_width="83dp"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>

            <TextView
                android:id="@+id/textViewCEPVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/cepVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <br.ufpb.lp3.gerenciamento_fiado.utils.CepMaskEditText
                android:id="@+id/editTextCEPVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewUFVendedorTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/ufVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <Spinner
                android:id="@+id/spinnerUFTelaAtualizarVendedor"
                android:layout_width="0dip"
                android:layout_height="38dp"
                android:layout_weight="0.15" />

            <TextView
                android:id="@+id/textViewCidaddeTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/cidadeVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextCidadeVendedorTelaAtualizarVendedor"
                android:layout_width="83dp"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutAtualizarVendedor10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewBairroTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dip"
                android:layout_marginTop="1dp"
                android:orientation="vertical"
                android:text="@string/bairroVendedor"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextBairroVendedorTelaAtualizarVendedor"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:ems="10"
                android:inputType="text" >

            </EditText>
        </LinearLayout>

        <!-- Parte 3 do layout -->

        <LinearLayout
            android:id="@+id/linearLayoutTelaLogin5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dip"
            android:gravity="center"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/buttonApagarTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/apagar" />

            <Button
                android:id="@+id/buttonAtualizarTelaAtualizarVendedor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/atualizarVendedor" />
        </LinearLayout>
    </LinearLayout>

</ScrollView>